<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="processes" />
                <binding name="locations" />
                <binding name="processes1" />
                <binding name="locations1" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8000/FocusProcessesOperations/ProcessesOperationService"
                binding="wsHttpBinding" bindingConfiguration="processes" contract="FocusProcessesService.IProcessesOperationsService"
                name="processes">
                <identity>
                    <servicePrincipalName value="host/mpx" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8000/FocusProcessesOperations/ProcessesOperationService"
                binding="wsHttpBinding" bindingConfiguration="locations" contract="FocusProcessesService.ILocationsOperationsService"
                name="locations">
                <identity>
                    <servicePrincipalName value="host/mpx" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8000/FocusProcessesOperations/ProcessesOperationService"
                binding="wsHttpBinding" bindingConfiguration="processes1"
                contract="FocusLocalizationsService.IProcessesOperationsService"
                name="processes1">
                <identity>
                    <servicePrincipalName value="host/mpx" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8000/FocusProcessesOperations/ProcessesOperationService"
                binding="wsHttpBinding" bindingConfiguration="locations1"
                contract="FocusLocalizationsService.ILocationsOperationsService"
                name="locations1">
                <identity>
                    <servicePrincipalName value="host/mpx" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>

  <system.diagnostics>
    <sources>
      <source name="System.ServiceModel.MessageLogging" switchValue="Warning, ActivityTracing">
        <listeners>
          <add name="ServiceModelTraceListener" />
        </listeners>
      </source>
      <source name="System.ServiceModel" switchValue="Verbose,ActivityTracing">
        <listeners>
          <add name="ServiceModelTraceListener" />
        </listeners>
      </source>
      <source name="System.Runtime.Serialization" switchValue="Verbose,ActivityTracing">
        <listeners>
          <add name="ServiceModelTraceListener" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add initializeData="App_tracelog.svclog" type="System.Diagnostics.XmlWriterTraceListener, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" name="ServiceModelTraceListener" traceOutputOptions="Timestamp" />
    </sharedListeners>
  </system.diagnostics>
</configuration>